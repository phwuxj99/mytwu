<h1>Listing games</h1>

<table class="tablecss">
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.name %></td>
    <td><%= post.title %></td>
    <td><%= link_to 'Show', post %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Game', new_post_path %>
